<p id="notice"><%= notice %></p>

<h1>Listing Morons</h1>

<table>
  <thead>
    <tr>
      <th>Image URL</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @morons.each do |moron| %>
      <tr>
        <td><%= link_to 'Link', moron.imgur %></td>
        <td><%= moron.description %></td>
        <td><%= link_to 'Show', moron %></td>
        <td><%= link_to 'Edit', edit_moron_path(moron) %></td>
        <!-- <td><%= link_to 'Destroy', moron, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Moron', new_moron_path %>
